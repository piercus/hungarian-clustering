{
  "name": "hungarian-clustering",
  "version": "1.0.0",
  "description": "Clustering using Hungarian Algorithm",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piercus/hungarian-clustering.git"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "keywords": [
    "cluster",
    "clustering",
    "hungarian"
  ],
  "author": "Pierre Colle <piercus@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/piercus/hungarian-clustering/issues"
  },
  "homepage": "https://github.com/piercus/hungarian-clustering#readme",
  "dependencies": {
    "munkres-js": "^1.2.2"
  },
  "xo": {
    "rules": {
      "unicorn/no-array-callback-reference": 0,
      "unicorn/no-array-reduce": 0,
      "unicorn/no-array-for-each": 0,
      "unicorn/prefer-module": 0,
      "import/extensions": 0
    }
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "semantic-release": "^17.4.7",
    "xo": "^0.44.0"
  }
}
